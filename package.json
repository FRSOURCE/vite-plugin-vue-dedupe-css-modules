{
  "private": true,
  "workspaces": [
    "packages/*",
    "example/*"
  ],
  "name": "@frsource/vite-plugin-vue-dedupe-css-modules-root",
  "description": "Vite plugin that removes duplicated CSS modules from your Vue JS bundle.",
  "scripts": {
    "prepare": "husky install",
    "lint": "yarn plugin:lint"
  },
  "license": "MIT",
  "homepage": "https://github.com/FRSOURCE/vite-plugin-vue-dedupe-css-modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FRSOURCE/vite-plugin-vue-dedupe-css-modules.git"
  },
  "bugs": {
    "url": "https://github.com/FRSource/vite-plugin-vue-dedupe-css-modules/issues"
  },
  "author": "Jakub Freisler <jakub.freisler@frsource.org>",
  "keywords": [
    "dedupe",
    "css modules",
    "vite",
    "vite-plugin",
    "vite-plugin-vue-dedupe-css-modules",
    "@frsource/vite-plugin-vue-dedupe-css-modules"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin main && npm publish"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@rollup/pluginutils": "^4.1.1",
    "@types/node": "^17.0.31",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "rollup": "^2.56.3",
    "standard-version": "^9.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "packageManager": "yarn@3.0.2"
}
